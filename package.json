{
  "name": "declareify",
  "version": "1.1.0",
  "description": "Browserify transform to declare undeclared module.exports variables",
  "main": "index.js",
  "scripts": {
    "build": "babel ./lib -d dist",
    "watch": "babel ./lib -w -d dist",
    "test": "babel-tape-runner test/*.js"
  },
  "keywords": [
    "browserify-transform",
    "module-exports",
    "declare",
    "variable"
  ],
  "author": "Aram Drevekenin",
  "license": "MIT",
  "dependencies": {
    "through2": "^2.0.0"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "babel-tape-runner": "^2.0.0",
    "browserify": "^13.0.0",
    "tape": "^4.4.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/imsnif/declareify"
  }
}
